citHeader("To cite cubfits in a publication use:")

citEntry(
  entry = "Misc",
  title = "{cubfits}: Codon Usage Bias Fits",
  author = personList(as.person("Wei-Chen Chen"),
                      as.person("Russell Zaretzki"),
                      as.person("William Howell"),
                      as.person("Cedric Landerer"),
                      as.person("Drew Schmidt"),
                      as.person("Michael A. Gilchrist")
                     ),
  year = "2014",
  note = "{R} Package, URL http://cran.r-project.org/package=cubfits",
  textVersion = "Chen, W.-C., Zaretzki, R., Howell, W., Landerer, C., Schmidt, D., and Gilchrist, M.A. (2014) cubfits: Codon Usage Bias Fits. R Package, URL http://cran.r-project.org/package=cubfits",
  key = "Chen2014cubfitspackage"
)

citEntry(
  entry = "Manual",
  title = "A Quick Guide for the {cubfits} Package",
  author = personList(as.person("Wei-Chen Chen"),
                      as.person("Russell Zaretzki"),
                      as.person("William Howell"),
                      as.person("Cedric Landerer"),
                      as.person("Drew Schmidt"),
                      as.person("Michael A. Gilchrist")
                     ),
  year = "2014",
  note = "{R} Vignette, URL http://cran.r-project.org/package=cubfits",
  textVersion = "Chen, W.-C., Zaretzki, R., Howell, W., Landerer, C., Schmidt, D., and Gilchrist, M.A. (2014) A Quick Guide for the cubfits Package. R Vignette, URL http://cran.r-project.org/package=cubfits",
  key = "Chen2014cubfitsvignette"
)
